{
  "version": 2,
  "buildCommand": "npm start",
  "installCommand": "npm install",
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/.*",
      "dest": "/index.js",
      "methods": ["POST", "GET", "PUT", "PATCH", "DELETE"]
    }
  ]
}
